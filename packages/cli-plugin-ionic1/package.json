{
  "name": "@ionic/cli-plugin-ionic1",
  "version": "0.0.6",
  "description": "Ionic CLI build plugin for Ionic 1 projects",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run clean && ../../node_modules/.bin/tsc",
    "clean": "rm -rf ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$",
    "setupFiles": [],
    "testResultsProcessor": "<rootDir>/../../node_modules/ts-jest/coverageprocessor.js"
  },
  "dependencies": {
    "@ionic/cli-utils": "^0.0.11",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "glob-watcher": "^3.1.0",
    "inquirer": "^3.0.6",
    "opn": "^4.0.2",
    "proxy-middleware": "^0.15.0",
    "tiny-lr": "^1.0.3",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/express": "^4.0.35",
    "@types/inquirer": "0.0.32",
    "@types/opn": "^3.0.28",
    "@types/xml2js": "0.0.33"
  }
}
