{
  "name": "@ionic/cli-plugin-ionic1",
  "version": "0.0.1",
  "description": "Ionic 1 plugin",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./src/index.js",
  "scripts": {
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$"
  },
  "dependencies": {
    "@ionic/app-generators": "0.0.3",
    "chalk": "^1.1.3",
    "cli-table": "0.3.1",
    "cross-spawn": "^5.0.1",
    "expand-tilde": "1.2.0",
    "form-data": "0.1.4",
    "gulp": "3.8.8",
    "gulp-util": "3.0.7",
    "inquirer": "0.11.2",
    "ionic-app-lib": "2.2.0",
    "moment": "2.11.1",
    "open": "0.0.5",
    "optimist": "0.6.0",
    "pretty-hrtime": "1.0.2",
    "progress": "1.1.7",
    "prompt": "0.2.12",
    "q": "1.0.1",
    "request": "2.74.0",
    "semver": "4.3.6",
    "serve-static": "1.7.1",
    "shelljs": "0.2.6",
    "underscore": "1.7.0",
    "unzip": "0.1.9"
  }
}
