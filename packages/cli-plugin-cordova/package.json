{
  "name": "@ionic/cli-plugin-cordova",
  "version": "0.0.11",
  "description": "Ionic Cordova Plugin for Ionic CLI",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./dist/index.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run clean && cp ./resources.json ./dist/resources.json && ../../node_modules/.bin/tsc",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch",
    "postinstall": "node ./bin/postinstall"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "files": [
    "bin/",
    "default-resources",
    "dist/",
    "resources.json"
  ],
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$",
    "testResultsProcessor": "<rootDir>/../../node_modules/ts-jest/coverageprocessor.js"
  },
  "dependencies": {
    "@ionic/cli-utils": "^0.0.10",
    "chalk": "^1.1.3",
    "inquirer": "^3.0.6",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/inquirer": "0.0.32",
    "@types/xml2js": "0.0.33",
    "chalk": "^1.1.3",
    "xml2js": "^0.4.17"
  }
}
