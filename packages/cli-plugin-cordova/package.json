{
  "name": "@ionic/cli-plugin-cordova",
  "version": "0.0.6",
  "description": "Ionic Cordova Plugin for Ionic CLI",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./dist/index.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run lint && npm run clean && ../../node_modules/.bin/webpack",
    "clean": "rm -rf ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch",
    "postinstall": "node ./bin/postinstall"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$"
  },
  "dependencies": {
    "@ionic/cli-utils": "^0.0.5",
    "node-fetch": "^1.6.3"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "form-data": "^2.1.2",
    "xml2js": "^0.4.17"
  },
  "peerDependencies": {
    "@ionic/app-scripts": "^1.1.0"
  },
  "files": [
    "bin/",
    "default-resources",
    "dist/",
    "resources.json"
  ]
}
