{
  "name": "@ionic/cli-plugin-core",
  "version": "0.0.1",
  "description": "Ionic CLI Core",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./dist/index.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run lint && npm run clean && ../../node_modules/.bin/tsc",
    "clean": "rm -rf ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@ionic/cli-utils": "^0.0.1",
    "@types/archiver": "^0.15.36",
    "chalk": "^1.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$"
  },
  "dependencies": {
    "@ionic/app-generators": "0.0.4",
    "archiver": "^1.3.0",
    "opn": "^4.0.2"
  }
}
